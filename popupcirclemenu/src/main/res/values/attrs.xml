<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="circlemenu">
        <attr name="button_color" format="color|reference" />
        <attr name="radius" format="dimension|reference" />
        <attr name="anim_duration" format="integer" />
        <attr name="open_direction">
            <enum name="RIGHT" value="1" />
            <enum name="LEFT" value="2" />
        </attr>

    </declare-styleable>

    <declare-styleable name="circlebutton">
        <attr name="pb_color" format="color|reference" />
        <attr name="pb_color_checked" format="color|reference" />
        <attr name="pb_background" format="reference" />
        <attr name="pb_background_checked" format="reference" />
    </declare-styleable>

</resources>